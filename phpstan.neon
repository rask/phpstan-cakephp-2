parameters:
    level: 8

    phpVersion:
        min: 80300
        max: 80599

    paths:
        - src
        - tests

    excludePaths:
        - tests/Feature/data/*
        - */Fixture/*
        # ported from cacke
        - src/CakePHP/PortedInflector.php

    ignoreErrors:
        # not practical as mixed
        - '#Method (.*?)::test(.*?)\(\) has parameter \$args with no type specified#'
